{
	"name": "uranio-adm",
	"version": "0.0.1",
	"description": "Uranio Admin",
	"main": "dist/index.js",
	"bin": {
		"uranio-generate-adm": "dist/server/generate.js",
		"uranio-webservice-adm": "dist/service/ws.js",
		"uranio-panel-adm": "dist/panel/index.js"
	},
	"exports": {
		".": "./dist/server.js",
		"./types": "./dist/server/types.js",
		"./client": "./dist/client.js",
		"./client/types": "./dist/client/types.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/server.d.ts"
			],
			"types": [
				"dist/server/types.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"client/types": [
				"dist/client/types.d.ts"
			]
		}
	},
	"repository": "git@github.com:nbl7/uranio-adm.git",
	"author": "Andrea Reni <mail@nbl7.net>",
	"scripts": {
		"postinstall": "yarn patch-package",
		"build": "yarn tsc -b",
		"dev:server": "yarn tsc-watch --onSuccess \"node -r source-map-support/register -r dotenv/config ./dist/dev.js\"",
		"dev:client": "yarn nuxt dev",
		"dev:panel": "yarn tsc-watch --onSuccess \"ts-node ./src/panel/index.ts\"",
		"dev:panel:dev": "yarn tsc-watch --onSuccess \"ts-node ./src/panel/index.ts dev\"",
		"dev:panel:build": "yarn tsc-watch --onSuccess \"ts-node ./src/panel/index.ts build\"",
		"dev:panel:generate": "yarn tsc-watch --onSuccess \"ts-node ./src/panel/index.ts generate\"",
		"dev:panel:start": "yarn tsc-watch --onSuccess \"ts-node ./src/panel/index.ts start\"",
		"dev": "yarn concurrently \"yarn dev:server\" \"yarn dev:client\"",
		"pre:build": "yarn esbuild src/server/generate.ts --bundle --platform=node --format=cjs --outfile=.generate/bundled_generate.js",
		"pre:generate": "node .generate/bundled_generate.js",
		"plain:generate": "node -r source-map-support/register -r dotenv/config ./dist/server/generate.js",
		"generate": "yarn plain:generate urn_schema_repo_path=.generate/uranio-schema urn_trx_repo_path=.generate/uranio-trx urn_repo_path=.generate",
		"generate:replace": "yarn plain:generate urn_schema_repo_path=node_modules/uranio-schema urn_trx_repo_path=node_modules/uranio-trx urn_repo_path=.",
		"generate:schema": "yarn generate urn_command=schema",
		"generate:hooks": "yarn generate urn_command=hooks",
		"generate:hook-types": "yarn generate urn_command=hook-types",
		"generate:client-config": "yarn generate urn_command=client-config"
	},
	"devDependencies": {
		"concurrently": "^7.0.0",
		"patch-package": "^6.4.7",
		"uranio-adm-dep-dev": "git+ssh://git@github.com/nbl7/uranio-adm-dep-dev.git#master"
	},
	"dependencies": {
		"@nuxt/cli": "^2.15.8",
		"@nuxtjs/proxy": "^2.1.0",
		"@uppy/audio": "^0.2.1",
		"@uppy/aws-s3": "^2.0.7",
		"@uppy/core": "^2.1.5",
		"@uppy/dashboard": "^2.1.4",
		"@uppy/image-editor": "^1.1.1",
		"@uppy/vue": "^0.4.5",
		"@uppy/webcam": "^2.0.5",
		"@uppy/xhr-upload": "^2.0.7",
		"dotenv": "^16.0.0",
		"esbuild": "^0.14.25",
		"minimist": "^1.2.5",
		"nuxt": "^2.15.8",
		"postinstall-postinstall": "^2.1.0",
		"saslprep": "^1.0.3",
		"uranio-api": "git+ssh://git@github.com/nbl7/uranio-api.git#master",
		"uranio-core": "git+ssh://git@github.com/nbl7/uranio-core.git#master",
		"uranio-schema": "ssh://git@github.com/nbl7/uranio-schema.git#master",
		"uranio-trx": "git+ssh://git@github.com/nbl7/uranio-trx.git#master",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git#master",
		"vuedraggable": "^2.24.3"
	}
}
