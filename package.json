{
	"name": "uranio-adm",
	"version": "0.0.1",
	"description": "Uranio Admin",
	"main": "dist/index.js",
	"exports": {
		".": "./dist/index.js",
		"./client": "./dist/client.js",
		"./types": "./dist/types.js",
		"./cln/types": "./dist/types.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"types": [
				"dist/types.d.ts"
			],
			"cln/types": [
				"dist/cln/types.d.ts"
			]
		}
	},
	"repository": "git@github.com:nbl7/uranio-adm.git",
	"author": "Andrea Reni <mail@nbl7.net>",
	"scripts": {
		"build": "yarn tsc -b",
		"generate:compile": "npx esbuild ./src/generate.ts --outfile=./generate/_generate_bundled.js --bundle --platform=node",
		"generate:run": "yarn generate:compile && node ./generate/_generate_bundled.js urn_output_dir=./generate urn_repo=adm",
		"generate:schema": "yarn generate:compile && node ./generate/_generate_bundled.js urn_output_dir=./.uranio/generate/types urn_repo=adm urn_command=schema",
		"generate:hooks": "yarn generate:compile && node ./generate/_generate_bundled.js urn_output_dir=./.uranio/generate urn_repo=adm urn_command=hooks",
		"generate:trx-types": "yarn generate:compile && node ./generate/_generate_bundled.js urn_output_dir=./.uranio/generate/types urn_repo=adm urn_command=trx-types",
		"dev:server": "yarn tsc-watch --onSuccess \"node -r source-map-support/register -r dotenv/config ./dist/dev.js\"",
		"dev:client": "yarn nuxt dev",
		"dev": "yarn concurrently \"yarn dev:server\" \"yarn dev:client\"",
		"dev:generate:compile": "yarn generate:compile --watch",
		"dev:generate:run": "npx nodemon --watch ./generate/_generate_bundled.js -e ts ./generate/_generate_bundled.js urn_output_dir=./generate urn_repo=adm",
		"dev:generate": "yarn concurrently \"yarn dev:generat:compile\" \"yarn dev:generate:run\"",
		"schema:reset": "yarn upgrade uranio-schema && npx npm-dts generate -L debug -r node_modules/uranio-schema -o ../../.uranio/generate/base/schema.d.ts",
		"trx-types:reset": "npx npm-dts generate -L debug -r ./node_modules/uranio-trx/ -o ../../.uranio/generate/base/uranio-trx.d.ts",
		"trx-types": "yarn trx-types:reset && yarn generate:trx-types"
	},
	"devDependencies": {
		"@nuxt/types": "^2.15.8",
		"@nuxt/typescript-build": "^2.1.0",
		"@nuxtjs/style-resources": "^1.2.1",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"concurrently": "^7.0.0",
		"esbuild": "^0.14.22",
		"pug": "^3.0.2",
		"pug-plain-loader": "^1.1.0",
		"sass": "^1.49.8",
		"sass-loader": "^10.1.1",
		"ts-loader": "^8.2.0",
		"uranio-dev": "git+ssh://git@github.com/nbl7/uranio-dev.git",
		"vue-typed-mixins": "^0.2.0"
	},
	"dependencies": {
		"@nuxtjs/proxy": "^2.1.0",
		"@uppy/audio": "^0.2.1",
		"@uppy/aws-s3": "^2.0.7",
		"@uppy/core": "^2.1.5",
		"@uppy/dashboard": "^2.1.4",
		"@uppy/image-editor": "^1.1.1",
		"@uppy/vue": "^0.4.5",
		"@uppy/webcam": "^2.0.5",
		"@uppy/xhr-upload": "^2.0.7",
		"dotenv": "^16.0.0",
		"nuxt": "^2.15.8",
		"uranio-api": "git+ssh://git@github.com/nbl7/uranio-api.git#new",
		"uranio-core": "git+ssh://git@github.com/nbl7/uranio-core.git#new",
		"uranio-schema": "ssh://git@github.com/nbl7/uranio-schema.git",
		"uranio-trx": "git+ssh://git@github.com/nbl7/uranio-trx.git#new",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git#new",
		"vuedraggable": "^2.24.3"
	}
}
