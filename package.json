{
	"name": "uranio-adm",
	"version": "0.0.1",
	"description": "Uranio Admin",
	"main": "dist/index.js",
	"bin": {
		"uranio-adm-generate": "dist/server/generate.js",
		"uranio-api": "dist/api.js"
	},
	"exports": {
		".": "./dist/server.js",
		"./types": "./dist/server/types.js",
		"./client": "./dist/client.js",
		"./client/types": "./dist/client/types.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/server.d.ts"
			],
			"types": [
				"dist/server/types.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"client/types": [
				"dist/client/types.d.ts"
			]
		}
	},
	"repository": "git@github.com:nbl7/uranio-adm.git",
	"author": "Andrea Reni <mail@nbl7.net>",
	"scripts": {
		"build": "yarn tsc -b",
		"generate:compile": "npx esbuild ./src/server/generate.ts --outfile=./generate/_generate_bundled.js --bundle --platform=node",
		"generate:run": "yarn generate:compile && node -r dotenv/config ./generate/_generate_bundled.js urn_output_dir=./generate urn_repo=adm",
		"generate:schema": "yarn generate:compile && node -r dotenv/config ./generate/_generate_bundled.js urn_repo=adm urn_command=schema",
		"generate:hooks": "yarn generate:compile && node -r dotenv/config ./generate/_generate_bundled.js urn_output_dir=./generate urn_repo=adm urn_command=hooks",
		"generate:hook-types": "yarn generate:compile && node -r dotenv/config ./generate/_generate_bundled.js urn_repo=adm urn_command=hook-types",
		"dev:server": "yarn tsc-watch --onSuccess \"node -r source-map-support/register -r dotenv/config ./dist/dev.js\"",
		"dev:client": "yarn nuxt dev",
		"dev": "yarn concurrently \"yarn dev:server\" \"yarn dev:client\""
	},
	"devDependencies": {
		"@nuxt/types": "^2.15.8",
		"@nuxt/typescript-build": "^2.1.0",
		"@nuxtjs/style-resources": "^1.2.1",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"concurrently": "^7.0.0",
		"esbuild": "^0.14.22",
		"pug": "^3.0.2",
		"pug-plain-loader": "^1.1.0",
		"sass": "^1.49.8",
		"sass-loader": "^10.1.1",
		"ts-loader": "^8.2.0",
		"uranio-dev": "git+ssh://git@github.com/nbl7/uranio-dev.git",
		"vue-typed-mixins": "^0.2.0"
	},
	"dependencies": {
		"@nuxtjs/proxy": "^2.1.0",
		"@uppy/audio": "^0.2.1",
		"@uppy/aws-s3": "^2.0.7",
		"@uppy/core": "^2.1.5",
		"@uppy/dashboard": "^2.1.4",
		"@uppy/image-editor": "^1.1.1",
		"@uppy/vue": "^0.4.5",
		"@uppy/webcam": "^2.0.5",
		"@uppy/xhr-upload": "^2.0.7",
		"dotenv": "^16.0.0",
		"nuxt": "^2.15.8",
		"saslprep": "^1.0.3",
		"uranio-api": "git+ssh://git@github.com/nbl7/uranio-api.git#update-modules",
		"uranio-core": "git+ssh://git@github.com/nbl7/uranio-core.git#update-modules",
		"uranio-schema": "ssh://git@github.com/nbl7/uranio-schema.git",
		"uranio-trx": "git+ssh://git@github.com/nbl7/uranio-trx.git#update-modules",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git#update-modules",
		"vuedraggable": "^2.24.3"
	}
}
